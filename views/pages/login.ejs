<% include ../partials/header %>
<div class="content-wrapper clearfix container row">
  <h1 class="content-header col m6 offset-m3 s12">Login</h1>
  <form id="login-form" action="login" method="post" class="col m6 offset-m3 s12">
    <div class="row">
      <div class="input-field col m12">
        <label for="email">Email</label>
        <input type="text" id="email" name="email" />
      </div>
    </div>
     <div class="row">
      <div class="input-field col m12">
        <label for="password">Password</label>
        <input type="password" id="password" name="password" />
      </div>
    </div>
    <div class="row">
      <button class="btn btn-large waves-effect col m10 offset-m1" type="submit" id="submit" name="submit">LOGIN</button>
    </div>
  </form>
</div>
<% include ../partials/footer %>